for _ in range(int(input())):
    m,n=map(int,input().split())
    a=list(map(int,input().split()))
    a=sorted(a)
    flag=0
    
    for i in range(m):
        for j in range(i+1,m):
            for k in range(j+1,m):
                if a[i]+a[j]+a[k] == n:
                    flag=1
                    
    if flag==1:
        print('1')
    else:
        print('0')
    
    
    
    
    
