#code
for _ in range(int(input())):
    input()
    l=list(map(int,input().split()))
    flag=0
    
    for i in range(len(l)):
        for j in range(i+1,len(l)):
            for k in range(j+1,len(l)):
                if l[i]+l[j]==l[k]:
                    flag=1
                    
if flag==1:
    print('1')
else:
    print('-1')
                    
                    
                
            
    
    

    
    
