"""

for _ in range(int(input())):
    m,n=input().split()
    a=list(map(int,input().split()))
    count=0
    flag=0
    for i in a:
        if i==int(n):
            count+=1
            flag=1
            
    if flag==1:
        print(count)
    else:
        print('-1')
    
    
