#code


for _ in range(int(input())):
    a=int(input())
    b=1
    c=1
    e=[]
    
    if a==1:
        print('1')
    elif a==2:
        print('1','1')
    else:
        for i in range(a-2):
            d=b+c
            b=c
            c=d
            e.append(d)
        e.append(1)
        e.append(1)
        print(*sorted(e))
        
